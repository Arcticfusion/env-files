# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = ${USERNAME}
  email = ${USER}@${HOST}
[pull]
	rebase = true
[alias]
  cp = cherry-pick
  co = checkout
  ch = checkout
  stat = status
  statb = status -sb
  ci = commit
  cm = commit -m
  cam = commit -am
  rmc = reset --hard HEAD~1
  last = log -l HEAD --stat
  ll = log HEAD~1..HEAD 
  logs = log --show-signature
  unstage = reset HEAD --
  back = reset --hard HEAD~ --
  untrack = rm --cached
[core]
  editor = vim
[init]
  defaultBranch = master
[credential]
	helper = cache --timeout 32400
